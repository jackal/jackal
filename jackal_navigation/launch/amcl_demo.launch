<launch>

  <!-- Run the map server -->
  <arg name="map_file" default="$(find jackal_navigation)/maps/willow-2010-02-18-0.10.yaml"/>
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  <!--- Run AMCL -->
  <include file="$(find jackal_navigation)/launch/amcl.launch" />

  <!--- Run Move Base -->
  <include file="$(find jackal_navigation)/launch/move_base.launch" />

</launch>
